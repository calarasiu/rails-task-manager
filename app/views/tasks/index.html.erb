<ul>
  <%@tasks.each do |task|%>
  <h1></h1>
  <p>Task</p>
  <li><%=task.title%></li>
  <%= link_to "Details", task_path(task)%>
  <%= link_to "Delete", task_path(task.id), method: :delete, data: { confirm: "Delete #{task.title?}" }%>
  <%end%>
</ul>

<%=link_to "Create a new task", tasks_new_path%>



